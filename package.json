{
  "name": "@garavest/eslint-config",
  "version": "2.1.0",
  "description": "A highly opinionated ESLint Config used by Garavest LLC for internal and public projects.",
  "keywords": [
    "eslint",
    "config",
    "svelte",
    "typescript",
    "prettier",
    "import"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/garavest/eslint-config.git"
  },
  "license": "MIT",
  "author": {
    "name": "Seth Murphy",
    "email": "seth.murphy@garavest.com",
    "url": "https://garavest.com"
  },
  "type": "module",
  "main": "index.js",
  "types": "index.d.ts",
  "files": [
    "lib/**/*",
    "index.js",
    "index.d.ts"
  ],
  "scripts": {
    "build": "tsc",
    "lint": "pnpm run lint:code && pnpm run lint:format && pnpm run lint:markdown",
    "lint:code": "eslint .",
    "lint:format": "prettier -c .",
    "lint:markdown": "markdownlint .",
    "prepare": "husky install",
    "release": "release-it",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "prettier": "@garavest/prettier-config",
  "dependencies": {
    "@eslint/js": "^8.55.0",
    "@stylistic/eslint-plugin": "^1.5.0",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-svelte": "^2.35.1",
    "globals": "^13.23.0",
    "svelte-eslint-parser": "^0.33.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@garavest/prettier-config": "^1.1.0",
    "@release-it/conventional-changelog": "^8.0.1",
    "@types/eslint": "^8.44.8",
    "@types/eslint__js": "^8.42.3",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "markdownlint": "^0.32.1",
    "markdownlint-cli": "^0.38.0",
    "release-it": "^17.0.1",
    "svelte": "^4.2.8",
    "typescript": "^5.3.3"
  }
}
