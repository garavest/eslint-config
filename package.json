{
  "name": "@garavest/eslint-config",
  "version": "2.0.0",
  "description": "A highly opinionated ESLint Config used by Garavest LLC for internal and public projects.",
  "keywords": [
    "eslint",
    "config",
    "svelte",
    "typescript",
    "prettier",
    "import"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/garavest/eslint-config.git"
  },
  "license": "MIT",
  "author": {
    "name": "Seth Murphy",
    "email": "seth.murphy@garavest.com",
    "url": "https://garavest.com"
  },
  "type": "module",
  "main": "index.js",
  "types": "index.d.ts",
  "files": [
    "lib/**/*",
    "index.js",
    "index.d.ts"
  ],
  "scripts": {
    "build": "tsc",
    "lint": "npm run lint:code && npm run lint:format",
    "lint:code": "eslint .",
    "lint:format": "prettier -c .",
    "prepare": "husky install",
    "release": "release-it",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "prettier": "@garavest/prettier-config",
  "dependencies": {
    "@eslint/js": "^8.50.0",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-svelte": "^2.33.2",
    "globals": "^13.22.0",
    "svelte-eslint-parser": "^0.33.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@garavest/prettier-config": "^1.0.1",
    "@release-it/conventional-changelog": "^7.0.2",
    "@types/eslint": "^8.44.3",
    "@types/eslint__js": "^8.42.1",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "markdownlint": "^0.31.1",
    "markdownlint-cli": "^0.37.0",
    "release-it": "^16.2.0",
    "svelte": "^4.2.1",
    "typescript": "^5.2.2"
  }
}
